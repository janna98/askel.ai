generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Employee {
  id                Int      @id @default(autoincrement())
  name              String   @unique
  department        String
  performanceReview String
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
}
